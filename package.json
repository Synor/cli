{
  "name": "@synor/cli",
  "description": "Synor Database Migration Tool",
  "version": "0.0.0",
  "author": "Munif Tanjim (https://muniftanjim.dev)",
  "bin": {
    "synor": "./bin/run"
  },
  "bugs": "https://github.com/Synor/cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1.2.2",
    "@oclif/plugin-help": "^2",
    "@synor/core": "^0.1.0",
    "lodash.defaultsdeep": "^4.6.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@synor/database-mysql": "^0.1.1",
    "@synor/source-file": "^0.1.1",
    "@types/lodash.defaultsdeep": "^4.6.6",
    "@types/node": "^10",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "globby": "^10",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Synor/cli",
  "keywords": [
    "synor",
    "synor-cli",
    "database",
    "migration",
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "synor",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Synor/cli",
  "scripts": {
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json",
    "test": "echo NO TESTS",
    "posttest": "eslint . --ext .ts --config .eslintrc.js",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
