{
  "name": "@synor/cli",
  "description": "CLI for Synor - Database Migration Tool",
  "version": "0.4.0",
  "author": "Munif Tanjim (https://muniftanjim.dev)",
  "bin": {
    "synor": "./bin/run"
  },
  "bugs": "https://github.com/Synor/cli/issues",
  "dependencies": {
    "@oclif/color": "^0.1.0",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1.2.2",
    "@oclif/plugin-help": "^2",
    "cli-ux": "^5.4.1",
    "lodash.defaultsdeep": "^4.6.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@synor/core": "^0.6.1",
    "@types/lodash.defaultsdeep": "^4.6.6",
    "@types/node": "^13.1.6",
    "@types/supports-color": "^5.3.0",
    "eslint": "^6.8.0",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "globby": "^11.0.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "ts-node": "^8",
    "typescript": "^3.7.4"
  },
  "peerDependencies": {
    "@synor/core": "^0.6.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Synor/cli",
  "keywords": [
    "cli",
    "synor",
    "database",
    "migration",
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "synor",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Synor/cli",
  "scripts": {
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && npm run readme",
    "postpack": "rm -f oclif.manifest.json",
    "test": "echo NO TESTS",
    "posttest": "eslint . --ext .ts --config .eslintrc.js",
    "readme": "oclif-dev readme",
    "postreadme": "pretty-quick --pattern '**/*.md'",
    "version": "npm run readme && pretty-quick && git add README.md"
  },
  "types": "lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  }
}
